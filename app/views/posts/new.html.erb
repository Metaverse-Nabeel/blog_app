<h2>Create a New Post</h2>

<%= form_with url: "/users/21/posts/create", model: @post do |form| %>
    <div>
        <%= form.label :Title %>
        <%= form.text_field :Title %>
    </div>
    <div>
        <%= form.label :Text %>
        <%= form.text_field :Text %>
    </div>
    <%= form.submit %>
<% end %>
